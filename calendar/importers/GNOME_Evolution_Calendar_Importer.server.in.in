                       _value="Evolution iCalendar importer"/>

</oaf_server>

<oaf_server iid="OAFIID:GNOME_Evolution_Calendar_vCalendar_Importer:@VERSION@"
	    type="factory"
	    location="OAFIID:GNOME_Evolution_Calendar_ImporterFactory:@VERSION@">

