msgid "Mail"
msgstr "E-Mail"

#: ../mail/importers/elm-importer.c:380
msgid "Evolution Elm importer"
msgstr "Evolution-Elm-Importeur"

#: ../mail/importers/elm-importer.c:381
msgid "Import mail from Elm."
msgstr "E-Mail von Elm importieren."

