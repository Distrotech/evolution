#: widgets/misc/e-dateedit.c:1307 widgets/misc/e-dateedit.c:1486
msgid "%H:%M"
msgstr "%H:%M"

#. strftime format of a time in 24-hour format.
#: e-util/e-time-utils.c:314
msgid "%H:%M:%S"
