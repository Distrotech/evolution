      </locale>
    </schema>


    <schema>
      <key>/schemas/apps/evolution/shell/reverse</key>
      <applyto>/apps/evolution/shell/reverse</applyto>
      <owner>evolution</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
        <short>Printer settings</short>
