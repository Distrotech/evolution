#include <bonobo.idl>

module GNOME {

	module Evolution {

		interface Shell : GNOME::Unknown {
			enum NewType {
				APPOINTMENT,
				MEETING_REQUEST,
				TASK,
				TASK_REQUEST,
				CONTACT,
				MAIL_MESSAGE,
				DISTRIBUTION_LIST,
				JOURNAL_ENTRY,
				NOTE
			};
			
			void new (in NewType type);
		};
	};
};

