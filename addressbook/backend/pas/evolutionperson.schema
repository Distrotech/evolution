	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

attributetype ( 666.666.666.1.6
	NAME 'managerName'
	SUP name )

attributetype ( 666.666.666.1.7
	NAME 'assistantName'
	SUP name )

