attributetype ( 666.666.666.1.14
	NAME 'evolutionArbitrary'
	EQUALITY caseIgnoreMatch
	SUBSTR caseIgnoreSubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{4096} )
	)

attributetype ( 666.666.666.1.15
	NAME 'fileAs'
	SUP name )

