	SYNTAX 1.3.6.1.4.1.1466.115.121.1.41 )

attributetype ( 666.666.666.1.10
	NAME ( 'mailer' 'mua' )
	EQUALITY caseIgnoreMatch
	SUBSTR caseIgnoreSubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{32} )

attributetype ( 666.666.666.1.11
	NAME ( 'birthDate' 'dob' )
